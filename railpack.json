{
  "$schema": "https://schema.railpack.com",
  "packages": {
    "node": "22",
    "yarn": "4.9.2"
  },
  "steps": {
    "install": {
      "commands": [
        "corepack enable",
        "yarn install --immutable"
      ]
    },
    "build": {
      "commands": [
        "yarn build"
      ]
    }
  },
  "deploy": {
    "startCommand": "yarn start"
  }
}
