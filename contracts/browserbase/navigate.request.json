{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://arcane-fly.github.io/z2/contracts/browserbase/navigate.request.json",
  "title": "browserbase.navigate.request",
  "description": "Browserbase navigation request",
  "type": "object",
  "additionalProperties": false,
  "required": ["url"],
  "properties": {
    "url": {
      "type": "string",
      "format": "uri",
      "description": "URL to navigate to"
    },
    "waitUntil": {
      "type": "string",
      "enum": ["load", "domcontentloaded", "networkidle0", "networkidle2"],
      "default": "load",
      "description": "Wait condition before returning"
    },
    "timeout": {
      "type": "integer",
      "minimum": 0,
      "maximum": 60000,
      "default": 30000,
      "description": "Navigation timeout in milliseconds"
    },
    "userAgent": {
      "type": "string",
      "description": "Custom user agent string"
    }
  }
}
