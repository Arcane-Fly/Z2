{
  "name": "z2-workspace",
  "private": true,
  "version": "0.1.0",
  "description": "Z2 AI Workforce Platform - Monorepo Workspace",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c",
  "scripts": {
    "build": "yarn workspaces foreach -A run build",
    "test": "yarn workspaces foreach -A run test",
    "dev": "yarn workspaces foreach -pi run dev",
    "lint": "yarn workspaces foreach -A run lint",
    "lint:fix": "yarn workspaces foreach -A run lint:fix",
    "type-check": "yarn workspaces foreach -A run type-check",
    "format": "yarn workspaces foreach -A run format",
    "format:check": "yarn workspaces foreach -A run format:check",
    "clean": "yarn workspaces foreach -A run clean",
    "install:all": "yarn install",
    "upgrade:all": "yarn up '*'",
    "constraints": "yarn constraints --fix"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.9.2"
  },
  "keywords": [
    "ai",
    "platform",
    "multi-agent",
    "llm",
    "orchestration",
    "mcp",
    "workspace"
  ],
  "license": "MIT",
  "author": "Z2 Team <team@z2.ai>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Arcane-Fly/Z2.git"
  },
  "bugs": {
    "url": "https://github.com/Arcane-Fly/Z2/issues"
  },
  "homepage": "https://github.com/Arcane-Fly/Z2#readme"
}
