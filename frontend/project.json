{
  "name": "frontend",
  "sourceRoot": "frontend/src",
  "projectType": "application",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "yarn build",
        "cwd": "{projectRoot}"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn dev",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn test",
        "cwd": "{projectRoot}"
      }
    },
    "test:ui": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn test:ui",
        "cwd": "{projectRoot}"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn test:coverage",
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn lint",
        "cwd": "{projectRoot}"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn lint:fix",
        "cwd": "{projectRoot}"
      }
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn type-check",
        "cwd": "{projectRoot}"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn format",
        "cwd": "{projectRoot}"
      }
    },
    "format:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn format:check",
        "cwd": "{projectRoot}"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn test:e2e",
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["type:app", "scope:frontend"]
}
